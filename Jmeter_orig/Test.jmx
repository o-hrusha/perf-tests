          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler - preparing variable as part of request body for confirming an order" enabled="true">
            <stringProp name="TestPlan.comments">Builds a multipart/form-data body with product_price fields and saves it to a  variable</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Get the productsMap object from JMeter variables
def productsMap = vars.getObject(&quot;productsMap&quot;)  // example: { &quot;115&quot;:&quot;75.00&quot;, &quot;70&quot;:&quot;345.00&quot; }

// Check if it is not empty
if (!productsMap || productsMap.isEmpty()) {
    throw new IllegalArgumentException(&quot;productsMap is empty or not found&quot;)
}

// Build the multipart/form-data body
def boundary = &quot;----WebKitFormBoundaryxBOsQOKEt8OMSRof&quot;
def sb = new StringBuilder()

productsMap.each { id, price -&gt;
    sb.append(&quot;${boundary}\n&quot;)
    sb.append(&quot;Content-Disposition: form-data; name=\&quot;product_price_${id}__\&quot;\n\n&quot;)
    sb.append(&quot;${price}\n&quot;)
}

// Add the closing boundary (sometimes required)
sb.append(&quot;${boundary}--\n&quot;)

def multipartBody = sb.toString()

// Save the result to a JMeter variable
vars.put(&quot;multipartBody&quot;, multipartBody)

// Log for verification
log.info(&quot;Multipart body:\n&quot; + multipartBody)
</stringProp>
