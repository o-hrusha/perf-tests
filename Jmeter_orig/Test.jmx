            <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler - prepearing body for placing an order" enabled="true">
              <stringProp name="TestPlan.comments">// Parses cart_content JSON from JMeter and extracts product IDs and quantities
// Builds a URL-encoded POST body including cart_content, p_id[], p_quantity[], total_net, trans_id, and shipping</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper
import java.net.URLEncoder

// --- 1. Get cart_content from JMeter variable ---
def cartContentStr = vars.get(&quot;cart_content&quot;) // example: {&quot;102__&quot;:1,&quot;70__&quot;:1}
def cartContent = new JsonSlurper().parseText(cartContentStr)

// --- 2. Prepare arrays for p_id[] and p_quantity[] ---
def p_ids = []
def p_quantities = []

cartContent.each { id, qty -&gt;
    p_ids &lt;&lt; id
    p_quantities &lt;&lt; qty.toString()
}

// --- 3. Build the POST string ---
def postParams = []

// cart_content (JSON URL-encoded)
def encodedCartContent = URLEncoder.encode(cartContentStr, &quot;UTF-8&quot;)
postParams &lt;&lt; &quot;cart_content=${encodedCartContent}&quot;

// p_id[] and p_quantity[]
p_ids.eachWithIndex { id, idx -&gt;
    postParams &lt;&lt; &quot;p_id[]=${URLEncoder.encode(id, &quot;UTF-8&quot;)}&quot;
    postParams &lt;&lt; &quot;p_quantity[]=${URLEncoder.encode(p_quantities[idx], &quot;UTF-8&quot;)}&quot;
}

// --- 4. Add other JMeter variables ---
def totalNet = vars.get(&quot;total_net&quot;)
def transId = vars.get(&quot;trans_id&quot;)
def shipping = &quot;order&quot; // fixed value

postParams &lt;&lt; &quot;total_net=${URLEncoder.encode(totalNet, &quot;UTF-8&quot;)}&quot;
postParams &lt;&lt; &quot;trans_id=${URLEncoder.encode(transId, &quot;UTF-8&quot;)}&quot;
postParams &lt;&lt; &quot;shipping=${URLEncoder.encode(shipping, &quot;UTF-8&quot;)}&quot;

def postBody = postParams.join(&quot;&amp;&quot;)

// --- 5. Save the result to a JMeter variable (renamed) ---
vars.put(&quot;PLACE_ORDER_BODY&quot;, postBody)

// --- 6. Log for verification ---
log.info(&quot;PLACE_ORDER_BODY = ${postBody}&quot;)
</stringProp>
            </JSR223Sampler>
