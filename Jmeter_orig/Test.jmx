              <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Ectratcor_product_id" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">def body = prev.getResponseDataAsString()

// Regex for product id
def regexId = ~/name=&quot;p_id\[\]&quot; class=&quot;product_id&quot; value=&quot;(.+?)&quot;/

// Regex for product price
def regexPrice = ~/class=&quot;edit-product-quantity&quot; data-p_id=&quot;(\d+)&quot; data-price=&quot;(.+?)&quot;/

// Dictionary to store {id: price}
def productsMap = [:]

// Get all ids
def matcherId = (body =~ regexId)
def ids = []
matcherId.each { m -&gt;
    ids &lt;&lt; m[1]
}

// Get all prices by id
def matcherPrice = (body =~ regexPrice)
matcherPrice.each { m -&gt;
    def p_id = m[1]
    def price = m[2]
    productsMap[p_id] = price
}

// Now productsMap contains { &quot;106&quot;: &quot;199.99&quot;, &quot;76&quot;: &quot;299.99&quot; }, etc.
vars.putObject(&quot;productsMap&quot;, productsMap)
log.info(&quot;productsMap: &quot; + productsMap)
</stringProp>
